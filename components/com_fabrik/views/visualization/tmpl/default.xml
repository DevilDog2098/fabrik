<?xml version="1.0" encoding="utf-8" standalone="no"?>
<metadata>
	<layout title="Visualization">
		<message>
			<![CDATA[Visualization]]>
		</message>
	</layout>
	<fields name="request">
		<fieldset addfieldprefix="Joomla\Component\Fabrik\Administrator\Field" name="request">
			<field default="0" description="" label="Select Instance" name="id" type="visualizationlist"/>
		</fieldset>
	</fields>
	<fields addfieldprefix="Joomla\Component\Fabrik\Administrator\Field" name="params">
		<fieldset label="COM_FABRIK_OPTIONS" name="params">
			<field default="0"
				class="switcher"
				description="COM_FABRIK_FIELD_RESET_FILTERS_DESC" label="COM_FABRIK_FIELD_RESET_FILTERS_LABEL" name="resetfilters" type="radio">
					<option value="0">JNO</option>
					<option value="1">JYES</option>
			</field>

			<field name="viz_extra_query_string"
				   type="text"
				   class="input-xlarge"
				   size="40"
				   label="COM_FABRIK_FIELD_EXTRA_QUERY_STRING_LABEL"
				   description="COM_FABRIK_FIELD_EXTRA_QUERY_STRING_DESC"
			/>
		</fieldset>
	</fields>
</metadata>
